end)

local ToggleInfiniteJump = Section:NewToggle("Infinite Jump", "Enables infinite jumping", function(state)
    local UserInputService = game:GetService("UserInputService")
    local Player = game.Players.LocalPlayer
    local Character = Player.Character or Player.CharacterAdded:Wait()
    local Humanoid = Character:WaitForChild("Humanoid")
    
    if _G.InfiniteJumpConnection then
        _G.InfiniteJumpConnection:Disconnect()
        _G.InfiniteJumpConnection = nil
    end
    
    if state then
        -- –ü–æ–¥–∫–ª—é—á–∞–µ–º —Å–æ–±—ã—Ç–∏–µ –Ω–∞ –∑–∞–ø—Ä–æ—Å –ø—Ä—ã–∂–∫–∞
        _G.InfiniteJumpConnection = UserInputService.JumpRequest:Connect(function()
            Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
        end)
        print("Infinite Jump enabled")
        
        -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
        Player.CharacterAdded:Connect(function()
            if state then
                task.wait(0.5) -- –ù–µ–±–æ–ª—å—à–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –ø–æ—Å–ª–µ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
                Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    else
        print("Infinite Jump disabled")
    end
end)

local hideFramesToggle = Section:NewToggle("Hide All Frames", "Hides all frame instances in ReplicatedStorage", function(state)
    local rSto = game:GetService("ReplicatedStorage")
    
    for _, obj in pairs(rSto:GetChildren()) do
        if obj:IsA("Instance") and obj.Name:match("Frame$") then
            obj.Visible = not state
        end
    end
    
    if state then
        print("All frames are now hidden")
    else
        print("All frames are now visible")
    end
end)

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Å–æ—Å—Ç–æ—è–Ω–∏—è –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
for _, obj in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
    if obj:IsA("Instance") and obj.Name:match("Frame$") then
        obj.Visible = true -- –£–±–µ–¥–∏—Ç–µ—Å—å, —á—Ç–æ –æ–Ω–∏ –≤–∏–¥–∏–º—ã, –µ—Å–ª–∏ –ø–µ—Ä–µ–∫–ª—é—á–∞—Ç–µ–ª—å –≤—ã–∫–ª—é—á–µ–Ω –∏–∑–Ω–∞—á–∞–ª—å–Ω–æ
    end
end

_G.InfiniteJumpConnection = nil

Section:NewToggle("Anti Lag", "–û–ø—Ç–∏–º–∏–∑–∏—Ä—É–µ—Ç –æ–±—ä–µ–∫—Ç—ã –¥–ª—è –ª—É—á—à–µ–π –ø—Ä–æ–∏–∑–≤–æ–¥–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏", function(state)
    antiLagEnabled = state
    
    if state then
        print("üåÄ Anti-Lag: –ê–ö–¢–ò–í–ò–†–û–í–ê–ù")
        
        -- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã—Ö –º–∞—Ç–µ—Ä–∏–∞–ª–æ–≤
        local originalMaterials = {}
        local originalReflectance = {}
        
        local function optimizeObjects()
            for _, v in pairs(Workspace:GetDescendants()) do
                if v:IsA("BasePart") and not v.Parent:IsA("Model") then
                    -- –°–æ—Ö—Ä–∞–Ω—è–µ–º –æ—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è
                    originalMaterials[v] = v.Material
                    originalReflectance[v] = v.Reflectance
                    
                    -- –ü—Ä–∏–º–µ–Ω—è–µ–º –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏–∏
                    v.Material = Enum.Material.SmoothPlastic
                    v.Reflectance = 0
                end
            end
        end
        
        local function restoreObjects()
            for part, material in pairs(originalMaterials) do
                if part:IsA("BasePart") then
                    part.Material = material
                    part.Reflectance = originalReflectance[part] or 0
                end
            end
            table.clear(originalMaterials)
            table.clear(originalReflectance)
        end
        
        -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –Ω–æ–≤—ã—Ö –æ–±—ä–µ–∫—Ç–æ–≤
        local descendantAddedConnection
        descendantAddedConnection = Workspace.DescendantAdded:Connect(function(descendant)
            if antiLagEnabled and descendant:IsA("BasePart") and not descendant.Parent:IsA("Model") then
                originalMaterials[descendant] = descendant.Material
                originalReflectance[descendant] = descendant.Reflectance
                descendant.Material = Enum.Material.SmoothPlastic
                descendant.Reflectance = 0
            end
        end)
        
        -- –ü–µ—Ä–≤–æ–Ω–∞—á–∞–ª—å–Ω–∞—è –æ–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è
        optimizeObjects()
        
        -- –§—É–Ω–∫—Ü–∏—è –¥–µ–∞–∫—Ç–∏–≤–∞—Ü–∏–∏
        local function deactivate()
            restoreObjects()
            if descendantAddedConnection then
                descendantAddedConnection:Disconnect()
            end
            print("üåÄ Anti-Lag: –î–ï–ê–ö–¢–ò–í–ò–†–û–í–ê–ù üö´")
        end
        
        return deactivate
        
    else
        print("üåÄ Anti-Lag: –î–ï–ê–ö–¢–ò–í–ò–†–û–í–ê–ù üö´")
    end
end)

local antiAFKEnabled = false
local afkConnection = nil

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ Anti AFK
local function activateAntiAFK()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/evxncodes/mainroblox/main/anti-afk", true))()
end

Section:NewToggle("Enable Anti AFK", "Prevents you from being kicked for AFK", function(state)
    antiAFKEnabled = state
    
    if state then
        activateAntiAFK()
        
        -- –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –∑–∞—â–∏—Ç–∞ –ø—Ä–∏ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏–∏
        game.Players.LocalPlayer.CharacterAdded:Connect(function()
            if antiAFKEnabled then
                task.wait(0.5)
                activateAntiAFK()
            end
        end)
    else
        -- –ú–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –æ—Ç–∫–ª—é—á–µ–Ω–∏–µ —Ñ—É–Ω–∫—Ü–∏–æ–Ω–∞–ª–∞, –µ—Å–ª–∏ —ç—Ç–æ –ø–æ–¥–¥–µ—Ä–∂–∏–≤–∞–µ—Ç—Å—è —Å–∫—Ä–∏–ø—Ç–æ–º
    end
end)

