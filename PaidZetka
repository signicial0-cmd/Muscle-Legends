                end
            end)
            
            -- –ü–µ—Ä–≤–æ–Ω–∞—á–∞–ª—å–Ω–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –¥–ª—è —Ç–µ–∫—É—â–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –¥–ª—è –Ω–æ–≤—ã—Ö –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–π
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        print("‚ùå –ê–≤—Ç–æ-—Ä–µ–∂–∏–º –¥–µ–∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω")
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end)

Section:NewToggle("Muscle King Rock (5.000.000 recomended size 5))", "–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –Ω–∞ —Ç–æ—á–∫—É –∫–∞–∂–¥—É—é —Å–µ–∫—É–Ω–¥—É", function(state)
    autoTeleport = state
    
    if state then
        print("üîµ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞ –∞–≤—Ç–æ-—Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è")
        
        local player = game.Players.LocalPlayer
        local targetCFrame = CFrame.new(-9029.7959, 5.53625202, -6026.74268, 0.276099414, -9.5099395e-09, -0.961129069, -3.1277569e-08, 1, -1.88795219e-08, 0.961129069, 3.52744038e-08, 0.276099414)
        
        -- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–π
        local connections = {}
        
        -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            local rootPart = character:WaitForChild("HumanoidRootPart")
            
            -- –§—É–Ω–∫—Ü–∏—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
            local function teleportToPoint()
                if autoTeleport and rootPart then
                    rootPart.CFrame = targetCFrame
                end
            end
            
            -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–∏
            connections.teleportLoop = game:GetService("RunService").Heartbeat:Connect(function()
                if autoTeleport then
                    teleportToPoint()
                end
            end)
            
            -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —Å–º–µ—Ä—Ç–∏
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- –û–∂–∏–¥–∞–Ω–∏–µ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
                if autoTeleport and player.Character then
                    teleportToPoint()
                end
            end)
            
            -- –ü–µ—Ä–≤–∞—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è
            teleportToPoint()
        end
        
        -- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –¥–ª—è —Ç–µ–∫—É—â–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –¥–ª—è –Ω–æ–≤—ã—Ö –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–π
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
        -- –§—É–Ω–∫—Ü–∏—è –æ—á–∏—Å—Ç–∫–∏
        return function()
            for _, conn in pairs(connections) do
                conn:Disconnect()
            end
        end
        
    else
        print("üî¥ –ê–≤—Ç–æ-—Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –¥–µ–∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞")
    end
end)

local autoMode = false
local connections = {}

Section:NewToggle("Auto Jungle Rock (10.000.000)", "–ê–≤—Ç–æ-—É–¥–∞—Ä—ã –ø–æ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º", function(state)
    autoMode = state
    
    if state then
        print("‚ö° –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω –∞–≤—Ç–æ-—Ä–µ–∂–∏–º —É–¥–∞—Ä–æ–≤ –ø–æ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º")
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(-7611.20166, 32.4031715, 2909.97168, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- –û—á–∏—Å—Ç–∫–∞ –ø—Ä–µ–¥—ã–¥—É—â–∏—Ö –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–π
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–º
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏
            local function equipPunch()
                local backpack = player:FindFirstChildOfClass("Backpack")
                if backpack then
                    local tool = backpack:FindFirstChild(toolName)
                    if tool and not character:FindFirstChild(toolName) then
                        tool.Parent = character
                        tool:Equip()
                        return true
                    end
                end
                return false
            end
            
            -- –§—É–Ω–∫—Ü–∏—è –∞—Ç–∞–∫–∏ —Å —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–µ–π
            local function startAttackLoop()
                if connections.attack then connections.attack:Disconnect() end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ü–µ–ª–∏
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- –ü–æ–∏—Å–∫ –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–∞ –¥–ª—è –∞—Ç–∞–∫–∏
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- –û–ø—Ç–∏–º–∞–ª—å–Ω–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É —É–¥–∞—Ä–∞–º–∏
                        end
                    end
                end)
            end
            
            -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–∞
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —Å–º–µ—Ä—Ç–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω–æ–µ –≤—Ä–µ–º—è –¥–ª—è –≥–∞—Ä–∞–Ω—Ç–∏—Ä–æ–≤–∞–Ω–Ω–æ–≥–æ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- –ü–µ—Ä–≤–æ–Ω–∞—á–∞–ª—å–Ω–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –¥–ª—è —Ç–µ–∫—É—â–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –¥–ª—è –Ω–æ–≤—ã—Ö –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–π
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        print("‚ùå –ê–≤—Ç–æ-—Ä–µ–∂–∏–º –¥–µ–∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω")
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end)

