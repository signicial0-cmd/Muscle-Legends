local Section = Tab:NewSection("")

Section:NewDropdown("SpeedChanger", "DropdownInf", {"Speed 100", "Speed 250", "Speed 500"}, function(qwerty)
    if qwerty == "Speed 100" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
elseif qwerty == "Speed 250" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 250
elseif qwerty == "Speed 500" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 500
end
end)

local Section = Tab:NewSection("")

Section:NewDropdown("JumpChanger", "DropdownInf", {"Jump 50", "Jump 200", "Jump 500"}, function(qwerty)
    if qwerty == "Jump 50" then
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
elseif qwerty == "Jump 200" then
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 200
elseif qwerty == "Jump 500" then
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 500
end
end)
