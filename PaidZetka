local Tab = Window:NewTab("Main")

local Section = Tab:NewSection("Rebirths Folder")

local toggleLoop = nil
Section:NewToggle("Auto Lift", "–û–ø–∏—Å–∞–Ω–∏–µ", function(state)
    if state then
        toggleLoop = task.spawn(function()
            while true do
                game.Players.LocalPlayer:WaitForChild("muscleEvent"):FireServer("rep")
                task.wait(0.1)
            end
        end)
    else
        if toggleLoop then
            task.cancel(toggleLoop)  -- –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ü–∏–∫–ª
            toggleLoop = nil
        end
    end
end)

Section:NewToggle("Auto Normal Rebirth", "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –¥–µ–ª–∞–µ—Ç —Ä–µ–±–∏—Ä—Ç", function(state)
    autoRebirthEnabled = state
    
    if state then
        print("‚ôªÔ∏è –ê–≤—Ç–æ-–†–µ–±–∏—Ä—Ç: –í–ö–õ (–∑–∞–ø—É—Å–∫ —Ü–∏–∫–ª–∞)")
        
        local player = game:GetService("Players").LocalPlayer
        local rebirthConnection
        local deathConnection

        -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —Ä–µ–±–∏—Ä—Ç–∞
        local function doRebirth()
            if autoRebirthEnabled then
                game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("rebirthRemote"):InvokeServer("rebirthRequest")
            end
        end

        -- –¶–∏–∫–ª —Ä–µ–±–∏—Ä—Ç–æ–≤ (—Å –∏–Ω—Ç–µ—Ä–≤–∞–ª–æ–º 0.1 —Å–µ–∫)
        rebirthConnection = game:GetService("RunService").Heartbeat:Connect(function()
            if autoRebirthEnabled then
                doRebirth()
            end
        end)

        -- –ê–≤—Ç–æ-–ø–µ—Ä–µ–ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏
        local function setupRespawn()
            if player.Character then
                deathConnection = player.Character.Humanoid.Died:Connect(function()
                    print("üíÄ –°–º–µ—Ä—Ç—å ‚Üí –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫ –∞–≤—Ç–æ-—Ä–µ–±–∏—Ä—Ç–∞ —á–µ—Ä–µ–∑ 0.5 —Å–µ–∫...")
                    task.wait(0.5)
                    if autoRebirthEnabled then
                        rebirthConnection = game:GetService("RunService").Heartbeat:Connect(doRebirth)
                    end
                end)
            end
        end

        setupRespawn()
        player.CharacterAdded:Connect(setupRespawn)
        
    else
        print("‚ôªÔ∏è –ê–≤—Ç–æ-–†–µ–±–∏—Ä—Ç: –í–´–ö–õ üö´")
        if rebirthConnection then rebirthConnection:Disconnect() end
        if deathConnection then deathConnection:Disconnect() end
    end
end)

Section:NewToggle("Auto Weight", "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –±–µ—Ä–µ—Ç –∏ –∫–∞—á–∞–µ—Ç Weight", function(state)
    autoWeightEnabled = state
    
    if state then
        print("‚öñÔ∏è –ê–≤—Ç–æ-–≤–µ—Å: –ê–ö–¢–ò–í–ò–†–û–í–ê–ù")
        
        local weightLoop
        local deathConnection
        local characterAddedConnection
        local player = game:GetService("Players").LocalPlayer
        
        -- –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏
        local function equipWeight()
            local character = player.Character
            if not character or not character:FindFirstChild("Humanoid") then return false end
            
            local humanoid = character:FindFirstChild("Humanoid")
            local weightTool = player.Backpack:FindFirstChild("Weight") or character:FindFirstChild("Weight")
            
            if weightTool and weightTool:IsA("Tool") then
                -- –ï—Å–ª–∏ Weight –≤ –±—ç–∫–ø–∞–∫–µ - —ç–∫–∏–ø–∏—Ä—É–µ–º
                if weightTool.Parent == player.Backpack then
                    humanoid:EquipTool(weightTool)
                    task.wait(0.2) -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ —ç–∫–∏–ø–∏—Ä–æ–≤–∫—É
                end
                return true
            end
            return false
        end
        
        -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –∞–∫—Ç–∏–≤–∞—Ü–∏–∏
        local function activateWeight()
            while autoWeightEnabled and task.wait(0.2) do
                if player.Character and player.Character:FindFirstChild("Humanoid") then
                    -- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ –ø—ã—Ç–∞–µ–º—Å—è —ç–∫–∏–ø–∏—Ä–æ–≤–∞—Ç—å (–Ω–∞ —Å–ª—É—á–∞–π, –µ—Å–ª–∏ –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç —É–ø–∞–ª)
                    equipWeight()
                    
                    local tool = player.Character:FindFirstChild("Weight")
                    if tool and tool:IsA("Tool") then
                        tool:Activate()
                    end
                end
            end
        end
        
        -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —Å–º–µ—Ä—Ç–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        local function onCharacterAdded(character)
            if deathConnection then deathConnection:Disconnect() end
            
            deathConnection = character:WaitForChild("Humanoid").Died:Connect(function()
                print("üíÄ –°–º–µ—Ä—Ç—å - –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫ —á–µ—Ä–µ–∑ 1.5 —Å–µ–∫...")
                if weightLoop then task.cancel(weightLoop) end
                
                task.wait(1.5) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω–æ–µ –≤—Ä–µ–º—è –¥–ª—è —Ä–µ—Å–ø–∞–≤–Ω–∞
                
                if autoWeightEnabled then
                    -- –î–≤–æ–π–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ—Å–ª–µ —Ä–µ—Å–ø–∞–≤–Ω–∞
                    task.wait(0.5)
                    equipWeight()
                    weightLoop = task.spawn(activateWeight)
                end
            end)
            
            if autoWeightEnabled then
                task.wait(1) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω–æ–µ –≤—Ä–µ–º—è –¥–ª—è –ø–æ–ª–Ω–æ–π –∑–∞–≥—Ä—É–∑–∫–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
                equipWeight()
                weightLoop = task.spawn(activateWeight)
            end
        end
        
        -- –ü–µ—Ä–≤–æ–Ω–∞—á–∞–ª—å–Ω–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞
        if player.Character then
            onCharacterAdded(player.Character)
        end
        
        -- –ü–æ–¥–∫–ª—é—á–∞–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ –Ω–æ–≤–æ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        characterAddedConnection = player.CharacterAdded:Connect(onCharacterAdded)
        
    else
        -- –î–µ–∞–∫—Ç–∏–≤–∞—Ü–∏—è
        print("‚öñÔ∏è –ê–≤—Ç–æ-–≤–µ—Å: –î–ï–ê–ö–¢–ò–í–ò–†–û–í–ê–ù üö´")
        if weightLoop then task.cancel(weightLoop) end
        if deathConnection then deathConnection:Disconnect() end
        if characterAddedConnection then characterAddedConnection:Disconnect() end
    end
end)

Section:NewToggle("Auto Size 2", "ToggleInfo", function(state)
    autoSizeEnabled = state
    
    if state then
        print("üìè –ê–≤—Ç–æ-—Ä–∞–∑–º–µ—Ä 2x: –ê–ö–¢–ò–í–ò–†–û–í–ê–ù (–æ–∂–∏–¥–∞–Ω–∏–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞)")
        
        local player = game.Players.LocalPlayer
        local sizeLoop
        local deathConnection

        -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞
        local function changeSize()
            while autoSizeEnabled do
                game:GetService("ReplicatedStorage").rEvents.changeSpeedSizeRemote:InvokeServer("changeSize", 2)
                task.wait()
            end
        end

        -- –ê–≤—Ç–æ-–ø–µ—Ä–µ–ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏
        local function setupRespawn()
            if player.Character then
                deathConnection = player.Character.Humanoid.Died:Connect(function()
                    print("üíÄ –°–º–µ—Ä—Ç—å ‚Üí –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫ –∏–∑–º–µ–Ω–µ–Ω–∏—è —Ä–∞–∑–º–µ—Ä–∞ —á–µ—Ä–µ–∑ 0.5 —Å–µ–∫...")
                    if sizeLoop then task.cancel(sizeLoop) end
                    task.wait(0.5)
                    if autoSizeEnabled then
                        sizeLoop = task.spawn(changeSize)
                    end
                end)
            end
        end

        -- –°—Ç–∞—Ä—Ç –æ—Å–Ω–æ–≤–Ω–æ–≥–æ —Ü–∏–∫–ª–∞
        sizeLoop = task.spawn(changeSize)
        setupRespawn()
        player.CharacterAdded:Connect(setupRespawn)
        
    else
        print("üìè –ê–≤—Ç–æ-—Ä–∞–∑–º–µ—Ä 2x: –î–ï–ê–ö–¢–ò–í–ò–†–û–í–ê–ù üö´")
        if sizeLoop then task.cancel(sizeLoop) end
        if deathConnection then deathConnection:Disconnect() end
    end
end)

local targetCFrame = CFrame.new(-8747.04199, 121.162392, -5858.64014, 0.66414386, -4.84184497e-08, 0.747604787, 7.34292414e-08, 1, -4.67000827e-10, -0.747604787, 5.52062112e-08, 0.66414386)
local teleportActive = false
local teleportThread

local function teleportPlayer()
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = targetCFrame
    end
end

local function toggleTeleport()
    teleportActive = not teleportActive
    
    if teleportActive then
        -- –ó–∞–ø—É—Å–∫–∞–µ–º —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—é
        teleportThread = coroutine.create(function()
            while teleportActive do
                teleportPlayer()
                wait(1)
            end
        end)
        coroutine.resume(teleportThread)
        
        -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
            if teleportActive then
                character:WaitForChild("HumanoidRootPart")
                teleportPlayer()
            end
        end)
        
        print("–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞ üîµ")
    else
        -- –û—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—é
        if teleportThread then
            coroutine.close(teleportThread)
        end
        print("–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –¥–µ–∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞ üî¥")
    end
end

-- –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫—É –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è
Section:NewButton("Auto King", "ButtonInfo", toggleTeleport)

local autoEquip = false
local connection = nil

local Section = Tab:NewSection("Kill stuff")

-- Auto Normal Punch (—Å –ø–æ–¥–¥–µ—Ä–∂–∫–æ–π —Ä–µ—Å–ø–∞–≤–Ω–∞)
local punchActive = false
local punchConnection = nil

Section:NewToggle("Auto Normal Punch", "–ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–µ –æ–±—ã—á–Ω—ã–µ —É–¥–∞—Ä—ã (—Ä–∞–±–æ—Ç–∞–µ—Ç –ø–æ—Å–ª–µ —Å–º–µ—Ä—Ç–∏)", function(state)
    punchActive = state
    
    -- –£–¥–∞–ª—è–µ–º –ø—Ä–µ–¥—ã–¥—É—â–µ–µ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ, –µ—Å–ª–∏ –±—ã–ª–æ
    if punchConnection then
        punchConnection:Disconnect()
        punchConnection = nil
    end
    
    if state then
        local player = game.Players.LocalPlayer
        
        local function startPunch()
            local char = player.Character or player.CharacterAdded:Wait()
            
            while punchActive and char and char:IsDescendantOf(workspace) do
                local punchTool = player.Backpack:FindFirstChild("Punch") or char:FindFirstChild("Punch")
                
                if punchTool then
                    -- –ê–≤—Ç–æ-—ç–∫–∏–ø–∏—Ä–æ–≤–∫–∞
                    if punchTool.Parent ~= char then
                        char:WaitForChild("Humanoid"):EquipTool(punchTool)
                        task.wait(0.1) -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ —ç–∫–∏–ø–∏—Ä–æ–≤–∫—É
                    end
                    
                    -- –ê–∫—Ç–∏–≤–∞—Ü–∏—è —É–¥–∞—Ä–∞ —Å –æ–±—ã—á–Ω–æ–π —Å–∫–æ—Ä–æ—Å—Ç—å—é
                    punchTool:Activate()
                end
                task.wait(0.35) -- –ò–Ω—Ç–µ—Ä–≤–∞–ª –º–µ–∂–¥—É —É–¥–∞—Ä–∞–º–∏ (–º–æ–∂–Ω–æ —Ä–µ–≥—É–ª–∏—Ä–æ–≤–∞—Ç—å)
            end
        end
        
        -- –ó–∞–ø—É—Å–∫–∞–µ–º —Å—Ä–∞–∑—É –∏ –ø—Ä–∏ –∫–∞–∂–¥–æ–º —Ä–µ—Å–ø–∞–≤–Ω–µ
        punchConnection = player.CharacterAdded:Connect(function()
            if punchActive then
                task.wait(0.5) -- –î–∞–µ–º –≤—Ä–µ–º—è –Ω–∞ –ø–æ—è–≤–ª–µ–Ω–∏–µ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
                startPunch()
            end
        end)
        
        startPunch() -- –ü–µ—Ä–≤—ã–π –∑–∞–ø—É—Å–∫
    end
end)

local autoKill = false
local connection = nil

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∞—Ç–∞–∫–∏
local function attackPlayers()
    local player = game.Players.LocalPlayer
    
    -- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –±—ã—Å—Ç—Ä–æ–π –∞—Ç–∞–∫–∏
    local punch = player.Backpack:FindFirstChild("Punch") or (player.Character and player.Character:FindFirstChild("Punch"))
    if punch and punch:FindFirstChild("attackTime") then
        punch.attackTime.Value = 0.065
    end
    
    -- –ê—Ç–∞–∫–∞ –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤
    for _, target in ipairs(game.Players:GetPlayers()) do
        if target ~= player then
            local root = target.Character and target.Character:FindFirstChild("HumanoidRootPart")
            local rHand = player.Character and player.Character:FindFirstChild("RightHand")
            local lHand = player.Character and player.Character:FindFirstChild("LeftHand")
            
 if root and rHand and lHand then
                firetouchinterest(rHand, root, 1)
                firetouchinterest(lHand, root, 1)
                firetouchinterest(rHand, root, 0)
                firetouchinterest(lHand, root, 0)
            end
        end
    end
end

Section:NewToggle("Auto Kill", "Automatically attacks all players", function(state)
    autoKill = state
    
    if connection then
        connection:Disconnect()
        connection = nil
    end
    
    if state then
        -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –∞—Ç–∞–∫–∏
        connection = game:GetService("RunService").Heartbeat:Connect(attackPlayers)
        
        -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
        game.Players.LocalPlayer.CharacterAdded:Connect(function()
            if autoKill then
                task.wait(0.5) -- –ù–µ–±–æ–ª—å—à–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –ø–æ—Å–ª–µ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
                attackPlayers()
            end
        end)
    end
end)

local Section = Tab:NewSection("")

-- –¢–µ–∫—Å—Ç–±–æ–∫—Å –¥–ª—è –≤–≤–æ–¥–∞ –Ω–∏–∫–∞ —Ü–µ–ª–∏
Section:NewTextBox("Target Player", "Enter the player's name", function(text)
    targetPlayerName = text
end)

Section:NewToggle("Auto Kill", "Automatically attacks the target player", function(state)
    autoKill = state
    
    if connection then
        connection:Disconnect()
        connection = nil
    end
    
    if state then
        -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –∞—Ç–∞–∫–∏
        connection = game:GetService("RunService").Heartbeat:Connect(attackPlayers)
        
        -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
        game.Players.LocalPlayer.CharacterAdded:Connect(function()
            if autoKill then
                task.wait(0.5) -- –ù–µ–±–æ–ª—å—à–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –ø–æ—Å–ª–µ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
                attackPlayers()
            end
        end)
    end
end)

local targetPlayerName = ""
local cameraConnection = nil
local cameraOffset = CFrame.new(0, 3, -8) -- –ö–∞–º–µ—Ä–∞ —Å–∑–∞–¥–∏ –∏ –Ω–µ–º–Ω–æ–≥–æ —Å–≤–µ—Ä—Ö—É

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –∫–∞–º–µ—Ä—ã
local function updateThirdPersonCamera()
    if not targetPlayerName or targetPlayerName == "" then return end
    
    local target = game.Players:FindFirstChild(targetPlayerName)
    if target and target.Character then
        local root = target.Character:FindFirstChild("HumanoidRootPart")
        if root then
            local camera = game.Workspace.CurrentCamera
            camera.CameraType = Enum.CameraType.Scriptable
            
            -- –ö–∞–º–µ—Ä–∞ —Å–ª–µ–¥—É–µ—Ç –∑–∞ –∏–≥—Ä–æ–∫–æ–º, –Ω–æ –æ—Å—Ç–∞–µ—Ç—Å—è –ø–æ–∑–∞–¥–∏ –Ω–µ–≥–æ
            local targetCFrame = root.CFrame
            camera.CFrame = targetCFrame * cameraOffset
        end
    end
end

local Section = Tab:NewSection("")

-- –¢–µ–∫—Å—Ç–±–æ–∫—Å –¥–ª—è –≤–≤–æ–¥–∞ –Ω–∏–∫–∞ —Ü–µ–ª–∏
Section:NewTextBox("Spy player", "Enter player name to track", function(text)
    -- –û—Ç–∫–ª—é—á–∞–µ–º –ø—Ä–µ–¥—ã–¥—É—â–µ–µ —Å–æ–µ–¥–∏–Ω–µ–Ω–∏–µ
    if cameraConnection then
        cameraConnection:Disconnect()
        cameraConnection = nil
    end
    
    targetPlayerName = text
    
    -- –í–∫–ª—é—á–∞–µ–º —Å–ª–µ–∂–µ–Ω–∏–µ, –µ—Å–ª–∏ –≤–≤–µ–¥–µ–Ω–æ –∏–º—è
    if text ~= "" then
        cameraConnection = game:GetService("RunService").RenderStepped:Connect(updateThirdPersonCamera)
    else
        -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –∫–∞–º–µ—Ä—É –≤ —Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–π —Ä–µ–∂–∏–º
        game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
    end
end)

-- –ö–Ω–æ–ø–∫–∞ –¥–ª—è –æ—Å—Ç–∞–Ω–æ–≤–∫–∏ —Å–ª–µ–∂–µ–Ω–∏—è
Section:NewButton("Stop spy player", "Return to normal camera", function()
    if cameraConnection then
        cameraConnection:Disconnect()
        cameraConnection = nil
    end
    game.Workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
    targetPlayerName = ""
end)

local Tab = Window:NewTab("Paid tab")

local Section = Tab:NewSection("")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏ —Ç–æ–ª—å–∫–æ "Swift Samurai" (–±–µ–∑ —Å–Ω—è—Ç–∏—è –Ω–∏—á–µ–≥–æ –¥–æ —ç—Ç–æ–≥–æ)
local function k()
    for m, n in pairs(c.petsFolder.Unique:GetChildren()) do
        if n.Name == "Swift Samurai" then
            a.rEvents.equipPetEvent:FireServer("equipPet", n)
        end
    end
end

-- –§–ª–∞–≥ –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Ü–∏–∫–ª–æ–º
local isFarming = false

Section:NewToggle("FAST STRENGTH (u need 7 new packs)", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
        k() -- –í—ã–∑–æ–≤ —Ñ—É–Ω–∫—Ü–∏–∏ –Ω–∞ –≤–∫–ª—é—á–µ–Ω–∏–µ —Ç—É–º–±–ª–µ—Ä–∞
        isFarming = true -- –£—Å—Ç–∞–Ω–æ–≤–∏—Ç–µ —Ñ–ª–∞–≥ –≤ true

        -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –¥–ª—è –±—ã—Å—Ç—Ä–æ–≥–æ –ø–æ–ª—É—á–µ–Ω–∏—è Strength
        task.spawn(function()
            while isFarming do -- –ü—Ä–æ–≤–µ—Ä—è–π—Ç–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ —Ñ–ª–∞–≥–∞
                for y = 1, 22 do
                    c.muscleEvent:FireServer("rep")
                end
                task.wait(0.001) -- –û—á–µ–Ω—å –±—ã—Å—Ç—Ä–æ
            end
        end)
    else
        print("Toggle Off")
        isFarming = false -- –£—Å—Ç–∞–Ω–æ–≤–∏—Ç–µ —Ñ–ª–∞–≥ –≤ false, —á—Ç–æ–±—ã –æ—Å—Ç–∞–Ω–æ–≤–∏—Ç—å —Ü–∏–∫–ª
    end
end)

local rebirthActive = false

Section:NewToggle("FAST REBIRTHS (u need 7 new packs)", "Fast Rebirths", function(state)
    if state then
        if not rebirthActive then
            rebirthActive = true
            loadstring(game:HttpGet("https://raw.githubusercontent.com/0o0o0o0o0o0o0o0o0o0o0o0o/0o0o0o0o/refs/heads/main/Kk"))()
            print("Fast Rebirths Active")
        end
    else
        if rebirthActive then
            rebirthActive = false
            -- –ó–¥–µ—Å—å –¥–æ–±–∞–≤—å—Ç–µ –∫–æ–¥ –¥–ª—è –±–µ–∑–æ–ø–∞—Å–Ω–æ–≥–æ –∑–∞–≤–µ—Ä—à–µ–Ω–∏—è –∏–ª–∏ –æ—Ç–º–µ–Ω—ã —ç—Ñ—Ñ–µ–∫—Ç–∞ Fast Rebirths, –µ—Å–ª–∏ —ç—Ç–æ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ
            print("Fast Rebirths Deactivated")
        end
    end
end)

local Tab = Window:NewTab("Glitching")

local Section = Tab:NewSection("Rocks")

local autoMode = false
local connections = {}

Section:NewToggle("Auto Tiny Rock (0)", "–ê–≤—Ç–æ-—Ä–µ–∂–∏–º —É–¥–∞—Ä–æ–≤", function(state)
    autoMode = state
    
    if state then
        print("–ê–≤—Ç–æ-—Ä–µ–∂–∏–º: –ê–ö–¢–ò–í–ï–ù üü¢ (Punch + —É–¥–∞—Ä—ã)")
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(17.6399956, -1.30998898, 2105.49121, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- –û—á–∏—Å—Ç–∫–∞ –ø—Ä–µ–¥—ã–¥—É—â–∏—Ö –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–π
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –¥–ª—è —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏ –∏ —É–¥–∞—Ä–æ–≤
        local function setupAttackSystem(character)
            -- –ñ–¥–µ–º –ø–æ–∫–∞ –ø–µ—Ä—Å–æ–Ω–∞–∂ –ø–æ–ª–Ω–æ—Å—Ç—å—é –∑–∞–≥—Ä—É–∑–∏—Ç—Å—è
            local humanoid = character:WaitForChild("Humanoid")
            
            -- –§—É–Ω–∫—Ü–∏—è —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏
            local function equipPunch()
                local tool = player.Backpack:FindFirstChild(toolName)
                if tool and not character:FindFirstChild(toolName) then
                    tool.Parent = character
                    tool:Equip()
                    return true
                end
                return false
            end
            
            -- –§—É–Ω–∫—Ü–∏—è —É–¥–∞—Ä–æ–≤
            local function startAttacking()
                if connections.attack then
                    connections.attack:Disconnect()
                end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ü–µ–ª–∏
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- –ü–æ–∏—Å–∫ –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–∞
                        local punchTool = character:FindFirstChildOfClass("Tool") or character:FindFirstChild(toolName)
                        if punchTool then
                            punchTool:Activate()
                            task.wait(0.01)
                        end
                    end
                end)
            end
            
            -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –ø–æ—è–≤–ª–µ–Ω–∏—è –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–∞
            local function onToolAdded(tool)
                if tool:IsA("Tool") or tool.Name == toolName then
                    startAttacking()
                end
            end
            
            -- –ü–æ–¥–∫–ª—é—á–∞–µ–º –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∏
            connections.toolAdded = character.ChildAdded:Connect(onToolAdded)
            connections.died = humanoid.Died:Connect(function()
                task.wait(0.5) -- –ñ–¥–µ–º –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
                if autoMode then
                    equipPunch()
                end
            end)
            
            -- –ü–µ—Ä–≤–æ–Ω–∞—á–∞–ª—å–Ω–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞
            if equipPunch() then
                startAttacking()
            end
        end
        
        -- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –¥–ª—è —Ç–µ–∫—É—â–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        if player.Character then
            setupAttackSystem(player.Character)
        end
        
        -- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –¥–ª—è –Ω–æ–≤—ã—Ö –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–π
        connections.characterAdded = player.CharacterAdded:Connect(setupAttackSystem)
        
    else
        print("–ê–≤—Ç–æ-—Ä–µ–∂–∏–º: –í–´–ö–õ–Æ–ß–ï–ù üî¥")
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end)

local autoMode = false
local connections = {}

Section:NewToggle("Auto Punching Rock (10)", "–ê–≤—Ç–æ-—É–¥–∞—Ä—ã –ø–æ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º", function(state)
    autoMode = state
    
    if state then
        print("‚ö° –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω –∞–≤—Ç–æ-—Ä–µ–∂–∏–º —É–¥–∞—Ä–æ–≤ –ø–æ –∫–æ–æ—Ä–¥–∏–Ω–∞—Ç–∞–º")
        
        local player = game.Players.LocalPlayer
        local toolName = "Punch"
        local targetCFrame = CFrame.new(-151.459747, 0.540015221, 422.589905, -1, 0, 0, 0, 1, 0, 0, 0, -1)
        
        -- –û—á–∏—Å—Ç–∫–∞ –ø—Ä–µ–¥—ã–¥—É—â–∏—Ö –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–π
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
        
        -- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–º
        local function setupCharacter(character)
            local humanoid = character:WaitForChild("Humanoid")
            
            -- –£–ª—É—á—à–µ–Ω–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è —ç–∫–∏–ø–∏—Ä–æ–≤–∫–∏
            local function equipPunch()
                local backpack = player:FindFirstChildOfClass("Backpack")
                if backpack then
                    local tool = backpack:FindFirstChild(toolName)
                    if tool and not character:FindFirstChild(toolName) then
                        tool.Parent = character
                        tool:Equip()
                        return true
                    end
                end
                return false
            end
            
            -- –§—É–Ω–∫—Ü–∏—è –∞—Ç–∞–∫–∏ —Å —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏–µ–π
            local function startAttackLoop()
                if connections.attack then connections.attack:Disconnect() end
                
                connections.attack = game:GetService("RunService").Heartbeat:Connect(function()
                    if autoMode and character and character.PrimaryPart then
                        -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è –∫ —Ü–µ–ª–∏
                        character:SetPrimaryPartCFrame(targetCFrame)
                        
                        -- –ü–æ–∏—Å–∫ –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–∞ –¥–ª—è –∞—Ç–∞–∫–∏
                        local tool = character:FindFirstChild(toolName) or character:FindFirstChildOfClass("Tool")
                        if tool then
                            tool:Activate()
                            task.wait(0.01) -- –û–ø—Ç–∏–º–∞–ª—å–Ω–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –º–µ–∂–¥—É —É–¥–∞—Ä–∞–º–∏
                        end
                    end
                end)
            end
            
            -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –∏–Ω—Å—Ç—Ä—É–º–µ–Ω—Ç–∞
            connections.toolAdded = character.ChildAdded:Connect(function(child)
                if child.Name == toolName or child:IsA("Tool") then
                    startAttackLoop()
                end
            end)
            
            -- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ —Å–º–µ—Ä—Ç–∏ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
            connections.died = humanoid.Died:Connect(function()
                task.wait(1.2) -- –£–≤–µ–ª–∏—á–µ–Ω–Ω–æ–µ –≤—Ä–µ–º—è –¥–ª—è –≥–∞—Ä–∞–Ω—Ç–∏—Ä–æ–≤–∞–Ω–Ω–æ–≥–æ –≤–æ–∑—Ä–æ–∂–¥–µ–Ω–∏—è
                if autoMode and player.Character then
                    equipPunch()
                end
            end)
            
            -- –ü–µ—Ä–≤–æ–Ω–∞—á–∞–ª—å–Ω–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞
            if equipPunch() then
                startAttackLoop()
            end
        end
        
        -- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –¥–ª—è —Ç–µ–∫—É—â–µ–≥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        if player.Character then
            setupCharacter(player.Character)
        end
        
        -- –ü–æ–¥–∫–ª—é—á–µ–Ω–∏–µ –¥–ª—è –Ω–æ–≤—ã—Ö –ø–µ—Ä—Å–æ–Ω–∞–∂–µ–π
        connections.characterAdded = player.CharacterAdded:Connect(setupCharacter)
        
    else
        print("‚ùå –ê–≤—Ç–æ-—Ä–µ–∂–∏–º –¥–µ–∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω")
        for _, conn in pairs(connections) do
            conn:Disconnect()
        end
        connections = {}
    end
end)

